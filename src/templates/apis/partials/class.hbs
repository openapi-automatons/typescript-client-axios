{{> comment api}}
export class {{api.title}} extends AbstractApi {
	readonly #config: {{api.title}}Config;

  /**
   * constructor
   * @param {Config} config
   */
	constructor(config: Config = {}) {
		super(config);
		this.#config = new {{api.title}}Config({{#if securities}}config.security{{/if}});
	}
	{{#each api.paths}}

	{{> apis/class/request }}
	{{/each}}
}
